from flask import Flask, render_template, request

app = Flask(__name__)



@app.route('/', methods=['GET', 'POST'])
def hello_world():
    if request.method == 'POST':
        return render_template('inndex.html')



    else:
        return render_template('inndex.html')

@app.route('/inner')
def hello():
    return render_template('inner-page.html')


if __name__ == '__main__':
    app.run()


